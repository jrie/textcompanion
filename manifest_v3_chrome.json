{
  "description": "Text-loving friend for the average internet user",
  "homepage_url": "https://github.com/jrie/textcompanion",
  "manifest_version": 3,
  "name": "TextCompanion",
  "version": "0.0.8",
  "permissions": ["storage", "activeTab"],
  "key": "",
  "options_ui": {
    "page": "page_settings_chrome.html",
    "browser_style": true
  },
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "textcompanion_bg.js"
  },
  "content_scripts": [{
    "matches": ["http://*/*", "https://*/*"],
    "js": ["textcompanion.js"],
    "css": ["textcompanion_chrome.css"]
  }],
  "web_accessible_resources": [
    {
      "resources": [
        "img/language-solid.svg",
        "img/magnifying-glass-solid.svg",
        "img/spell-check-solid.svg",
        "img/square-arrow-up-right-solid.svg",
        "img/cart-shopping-solid.svg"
      ],
      "matches": [ "<all_urls>" ]
    }
  ]
}
